    "axios": "^1.6.7",
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
